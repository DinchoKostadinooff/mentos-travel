<h3 class="text-center">Create your own holiday</h3>
<br  />
<div class="row">
	<div class="col-md-2 text-center">
    	<label for="from">Flying from:</label>
      	<select class="form-control" id="from" >
            <option>London</option>
            <option>Bristol</option>
            <option>Cardiff</option>
            <option>Luton</option>
            <option>Southend</option>
            <option>Exeter</option>
            <option>Edinburgh</option>
            <option>Liverpool</option>
            <option>Newcastle</option>
            <option>Norwich</option>
            <option>Doncaster Sheffield</option>
            <option>Bournemouth</option>
            <option>Leeds</option>
            <option>Southampton</option>
            <option>Glasgow</option>
            <option>Manchester</option>
            <option>Belfast City</option>
            <option>Humberside</option>
      	</select>
    </div>
    <div class="col-md-2 text-center">
    	<label for="to">To:</label>
      	<select class="form-control" id="to" >
            <option>Sofia</option>
            <option>Plovdiv</option>
            <option>Burgas</option>
            <option>Varna</option>
      	</select>
    </div>
    <div class="col-md-2 text-center">
    	<label for="date">Date:</label>
    	<input type="text" id="datepicker" class="form-control" id="dates">
    </div>
    <div class="col-md-2 text-center">
    	<label for="days">Days:</label>
    	<input type="number" class="form-control" id="days" min="1" max="10">
    </div>
    <div class="col-md-1 text-center">
    	<label for="adults">Adults:</label>
      	<select class="form-control" id="adults" >
            <option>1</option>
            <option>2</option>
            <option>3</option>
            <option>4</option>
      	</select>
    </div>
    <div class="col-md-1 text-center">
    	<label for="children">Children:</label>
      	<select class="form-control" id="children" >
            <option>1</option>
            <option>2</option>
            <option>3</option>
            <option>4</option>
      	</select>
    </div>
    <div class="col-md-2 text-center">
    	<label for="destination">Destination:</label>
      	<select class="form-control" id="destination" >
            <option>Sofia</option>
            <option>Sunny Beach</option>
            <option>Varna</option>
            <option>Bansko</option>
            <option>Burgas</option>
            <option>Nesebar</option>
            <option>Sozopol</option>
            <option>Veliko Tarnovo</option>
            <option>Albena</option>
            <option>Borovets</option>
            <option>Pamporovo</option>
            <option>Plovdiv</option>
            <option>Balchik</option>
            <option>Vidin</option>
            <option>Sveti Vlas</option>
            <option>Obzor</option>
            <option>Vitosha</option>
            <option>Devin</option>
            <option>Pomorie</option>
            <option>Kavarna</option>
            <option>Sandanski</option>
            <option>Rila</option>
            <option>Sliven</option>
      	</select>
    </div>
    </div>
</div>
<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
      <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
      <li data-target="#myCarousel" data-slide-to="1"></li>
      <li data-target="#myCarousel" data-slide-to="2"></li>
      <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner" role="listbox">
      <div class="item active">
        <img src="img_chania.jpg" alt="Chania" width="460" height="345">
      </div>

      <div class="item">
        <img src="img_chania2.jpg" alt="Chania" width="460" height="345">
      </div>
    
      <div class="item">
        <img src="images/Borovets.jpg" alt="Flower" width="460" height="345">
      </div>

      <div class="item">
        <img src="img_flower2.jpg" alt="Flower" width="460" height="345">
      </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev">
      <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next">
      <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
